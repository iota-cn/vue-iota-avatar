{
    "name": "@iota-cn/avatar",
    "version": "0.1.12",
    "main": "./dist/avatar.common.js",
    "license": "MIT",
    "keywords": [
        "@iota",
        "vue",
        "components",
        "avatar-viewer",
        "avatar-editor",
        "avatar"
    ],
    "scripts": {
        "examples": "vue-cli-service serve ./examples/main.js",
        "build": "vue-cli-service build --target lib --name avatar ./src/index.js",
        "lint": "vue-cli-service lint"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:iota-cn/vue-iota-avatar.git"
    },
    "dependencies": {},
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.1.1",
        "@vue/cli-plugin-eslint": "^4.1.1",
        "@vue/cli-service": "^4.1.1",
        "babel-eslint": "^10.0.3",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.0.1",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.2",
        "vue": "^3.6.10",
        "vue-template-compiler": "^2.6.11",
        "webpack-bundle-analyzer": "^3.6.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
